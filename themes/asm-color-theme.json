{
	"name": "Assembler",
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment.line.semi-colon"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Immediate",
			"scope": [
				"immediate.asm",
			],
			"settings": {
				"foreground": "#a3a000",
				"fontStyle": "italic",
			}
		},
		{
			"name": "Value",
			"scope": [
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary",
				"constant.numeric.decimal",
			],
			"settings": {
				"foreground": "#f0ec00"
			}
		},
		{
			"name": "Address",
			"scope": [
				"entity.name.section"
			],
			"settings": {
				"foreground": "#ffae00",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Instruction",
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#cc00ff",
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Userdefined",
			"scope": [
				"meta.tag",
			],
			"settings": {
				"foreground": "#ff00dd",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Directives",
			"scope": [
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#7dffa8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "VariablePreProcessor",
			"scope": [
				"meta.parameter.type.variable"
			],
			"settings": {
				"foreground": "#89fad8",
			}
		},
		{
			"name": "Label",
			"scope": [
				"meta.label",
				"meta.storage.label"
			],
			"settings": {
				"foreground": "#4ac6ff",
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "Definitions",
			"scope": [
				"constant",
				"constant.asm",
				"address.asm",
				"meta.constant"
			],
			"settings": {
				"foreground": "#4ac6ff"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted",
			],
			"settings": {
				"foreground": "#00a71c"
			}
		},
		{
			"name": "Path",
			"scope": [
				"string.path"
			],
			"settings": {
				"foreground": "#fd73d4",
				"fontStyle": "underline"
			}
		}
	]
}
