{
	"semanticHighlighting": true,
	"semanticTokenColors": {},
	"name": "VASM (oldstyle)",
	"type": "dark",
	"colors": {
		"editor.background": "#263238",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Imidiate",
			"scope": [
				"imidiate.asm",
			],
			"settings": {
				"foreground": "#a3a000",
				"fontStyle": "italic",
			}
		},
		{
			"name": "Value",
			"scope": [
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary",
				"constant.numeric.decimal",
			],
			"settings": {
				"foreground": "#f0ec00"
			}
		},
		{
			"name": "Address",
			"scope": [
				"address.asm",
			],
			"settings": {
				"foreground": "#ffae00",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Instruction",
			"scope": [
				"instructionname.asm",
			],
			"settings": {
				"foreground": "#cc00ff",
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Userdefined",
			"scope": [
				"userdefined.asm",
			],
			"settings": {
				"foreground": "#ff00dd",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Directives",
			"scope": [
				"directives.asm"
			],
			"settings": {
				"foreground": "#7dffa8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "VariablePreProcessor",
			"scope": [
				"variable.pp"
			],
			"settings": {
				"foreground": "#89fad8",
			}
		},
		{
			"name": "Label",
			"scope": [
				"reference.label.asm",
				"storage"
			],
			"settings": {
				"foreground": "#4ac6ff",
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "Definitions",
			"scope": [
				"reference.constant.asm",
				"reference.macro.asm"
			],
			"settings": {
				"foreground": "#4ac6ff"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted",
			],
			"settings": {
				"foreground": "#00a71c"
			}
		},
		{
			"name": "Path",
			"scope": [
				"string.path"
			],
			"settings": {
				"foreground": "#fd73d4",
				"fontStyle": "underline"
			}
		}
	]
}
